<Window x:Class="ProyectoDAI_1.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProyectoDAI_1"
        mc:Ignorable="d"
        Title="Login" Height="200" Width="300" WindowStartupLocation="CenterScreen">
    <Grid Background="#441B1B1B">
        <Label Content="Usuario" HorizontalAlignment="Left" Margin="32,32,0,0" VerticalAlignment="Top"/>
        <Label Content="Contraseña" HorizontalAlignment="Left" Margin="32,87,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="TxUsuario" HorizontalAlignment="Left" Height="23" Margin="141,32,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="TxPasswd" HorizontalAlignment="Left" Height="23" Margin="141,87,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button Content="Volver" HorizontalAlignment="Left" Margin="186,133,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Button Content="Iniciar sesión" HorizontalAlignment="Left" Margin="30,133,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <Button Content="Contacto" HorizontalAlignment="Left" Margin="108,133,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_2"/>

    </Grid>
</Window>
